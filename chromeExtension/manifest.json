{
  "manifest_version": 2,

  "name": "Pickle",
  "description": "This extension allows the user to leave comments on any website",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}